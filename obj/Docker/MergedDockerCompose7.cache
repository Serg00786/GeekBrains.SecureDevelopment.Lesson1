services:
  geekbrains.securedevelopment.lesson1:
    build:
      context: C:\Users\HomePC\source\repos\GeekBrains.SecureDevelopment.Lesson1\GeekBrains.SecureDevelopment.Lesson1
      dockerfile: GeekBrains. SecureDevelopment. Lesson1/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: GeekBrains.SecureDevelopment.Lesson1
      target: base
    container_name: GeekBrains.SecureDevelopment.Lesson1
    depends_on:
      postgres:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionStrings__Default: Host=postgres;DataBase=postgrdb;User Id=user;Password=123;
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: geekbrainssecuredevelopmentlesson1:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/GeekBrains.SecureDevelopment.Lesson1.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - C:\Users\HomePC\source\repos\GeekBrains.SecureDevelopment.Lesson1\GeekBrains.SecureDevelopment.Lesson1\GeekBrains.
      SecureDevelopment. Lesson1:/app:rw
    - C:\Users\HomePC\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\HomePC\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\HomePC\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\HomePC\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\HomePC\source\repos\GeekBrains.SecureDevelopment.Lesson1\GeekBrains.SecureDevelopment.Lesson1:/src:rw
  postgres:
    environment:
      POSTGRES_DB: postgrdb
      POSTGRES_PASSWORD: '123'
      POSTGRES_USER: user
    image: postgres:13.3
    ports:
    - published: 5433
      target: 5432
version: '3.4'